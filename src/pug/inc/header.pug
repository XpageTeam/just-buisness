header.head
    .wrapper
        .head__wrap
            .head__logo
                figure.logo
                    img(src="img/pb_logo.svg")
                    img(src="img/logo.png")
            nav.head__menu
                ul.menu
                    each link in [{title: "О проекте", id: "about"}, {title: "Наставники", id: "mentors"}, {title: "Возможности", id: "apport"}, {title: "3 сезон", id: "show"}, {title: "Партнеры", id: "partners"}, {title: "Контакты", id: "contacts"}, ]
                        li
                            a.menu__link.scrollto(href="#"+link.id)=link.title
            .head__btn
                a.default-btn.scrollTo(href="#request") Стать участником